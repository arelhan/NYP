@startuml Inheritance
title Vehicle

class Vehicle{
    #manifacturer : String
    #year : int
    #tireHealt
    #color : String
    +start()
    +stop()
    +drive()
}
class Car extends Vehicle{
    +openSunroof()

}
class Truck extends Vehicle{
    +transport()

}
class Bus extends Vehicle{
    +scheduling()

}
class SchoolBus extends Bus{
    +getdare()
}
@enduml